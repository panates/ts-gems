{
  "extends": "@panates/tsconfig/node.json",
  "compilerOptions": {
    "types": ["node", "jest"],
    "module": "CommonJS",
    "target": "ES6",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": false
  },
  "include": ["src", "test"],
  "exclude": ["node_modules"]
}
